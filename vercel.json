{
  "version": 2,
  "builds": [
    {
      "src": "dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/$1"
    },
    {
      "src": "/",
      "dest": "/dist/index.html"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)\\.(js|css|ico|jpg|jpeg|png|gif|webp|svg|woff|woff2|ttf|eot)",
      "dest": "/dist/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/index.html"
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm run build",
  "framework": "nextjs",
  "public": true
}
